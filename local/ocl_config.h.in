#ifndef config_h
#define config_h

#undef PACKAGE_NAME
#undef PACKAGE_TARNAME
#undef PACKAGE_VERSION
#undef PACKAGE_STRING
#undef PACKAGE_BUGREPORT
#undef PACKAGE_URL
#undef PACKAGE
#undef VERSION

#undef STDC_HEADERS
#undef HAVE_SYS_TYPES_H
#undef HAVE_SYS_STAT_H
#undef HAVE_STDLIB_H
#undef HAVE_STRING_H
#undef HAVE_MEMORY_H
#undef HAVE_STRINGS_H
#undef HAVE_INTTYPES_H
#undef HAVE_STDINT_H
#undef HAVE_UNISTD_H
#undef HAVE_DLFCN_H
#undef LT_OBJDIR

/*----------------------------------------------------------------------------*
 * Configured values: These should be set by the configure script.
 *----------------------------------------------------------------------------*/

/* define the platform to be used for the performance device */
#undef OCL_PLATFORM_PERFORMANCE

/* define the function to be used for the performance device initialization */
#undef OCL_PERFORMANCE_INITIALIZATION

/* define the platform to be used for the auxiliary device */
#undef OCL_PLATFORM_AUXILIARY

/* define the function to be used for the auxiliary device initialization */
#undef OCL_AUXILIARY_INITIALIZATION

/* flag used by the fortran compiler to search for modules */
#undef FC_MODULE_PATH_FLAG

/* enable profiling information when creating queues */
#undef ENABLE_OPENCL_PROFILING

/* enable verbose output */
#undef ENABLE_OCL_VERBOSE

/* enable assertions: these will print a message and exit on error */
#undef ENABLE_OCL_ASSERTIONS

/* strdup function */
#undef HAVE_DECL_STRDUP

/* strndup function */
#undef HAVE_DECL_STRNDUP

/* strsep function */
#undef HAVE_DECL_STRSEP

/*----------------------------------------------------------------------------*
 * Defaulted values: These do not need to be set unless you want to
 * override them.
 *----------------------------------------------------------------------------*/

/* granularity of event list block allocation */
#undef OCL_EVENT_LIST_BLOCK_SIZE

/* maximum source length allowed for kernel sources */
#undef OCL_MAX_SOURCE_LENGTH

/* maximum number of memory allocations for garbage collection */
#undef OCL_MAX_ALLOCATIONS

/* maximum number of logical devices */
#undef OCL_MAX_LOGICAL_DEVICES

/* maximum number of platforms */
#undef OCL_MAX_PLATFORMS

/* maximum number of devices per platform */
#undef OCL_MAX_PLATFORM_DEVICES

#endif // config_h
